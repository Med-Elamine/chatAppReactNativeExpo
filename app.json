{
  "expo": {
    "name": "Chat With Me!",
    "description": "Here we can chat & share our ideas",
    "slug": "chatAppReactNativeExpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/chatSplash.png",
    "splash": {
      "image": "./assets/chatSplash.png",
      "resizeMode": "contain",
      "width": "auto",
      "height": "25%",
      "backgroundColor": "#ffffff",
      "plugins": [
        [
          "expo-image-picker",
          {
            "photosPermission": "The app accesses your photos to let you share them with your friends."
          }
        ]
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
